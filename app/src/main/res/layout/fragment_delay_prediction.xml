<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:eaze="http://schemas.android.com/tools">
    <data>
        <import type="com.java.flightscheduler.utils.ParsingUtils"/>
        <variable
            name="predictionViewModel"
            type="com.java.flightscheduler.ui.delayprediction.predictionresults.DelayPredictionViewModel" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.github.mikephil.charting.charts.PieChart
            app:setChartVariables="@{predictionViewModel.delayPrediction}"
            android:id="@+id/chartDelayPrediction"
            eaze:egHighlightStrength="1"
            eaze:egInnerPadding="0"
            android:layout_width="match_parent"
            android:layout_height="@dimen/detail_window_hotel"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>